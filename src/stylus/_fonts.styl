@font-face {
  font-family: 'DIN Pro Cond';
  src: url('../fonts/subset-DINPro-CondensedBold.eot');
  src: local('DIN Pro Condensed Bold'), local('DINPro-CondensedBold'),
          url('../fonts/subset-DINPro-CondensedBold.eot?#iefix') format('embedded-opentype'),
          url('../fonts/subset-DINPro-CondensedBold.woff2') format('woff2'),
          url('../fonts/subset-DINPro-CondensedBold.woff') format('woff'),
          url('../fonts/subset-DINPro-CondensedBold.ttf') format('truetype');
  font-weight: bold;
  font-style: normal;
}

